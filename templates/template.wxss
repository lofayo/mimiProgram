/* templates/template.wxss */
.movie_surface {
  width: 100%;
  height: 320rpx;
  margin-bottom: 8rpx;
}
.movie_title {
  display: inline-block;
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.comment {
  margin-top: 8rpx;
}
.star {
  width: 16rpx;
  height: 16rpx;
}
.movie_item {
  width: 33.3%;
  float: left;
  padding-right: 24rpx;
  box-sizing: border-box;
  margin-bottom: 36rpx;
}